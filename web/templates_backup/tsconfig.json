{
  "compilerOptions": {
    "target": "ES2017",
    "module": "ESNext",
    "outDir": "./scripts",
    "rootDir": "./typescript",
    "lib": ["DOM", "ES2017"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowUmdGlobalAccess": true,
    "jsx": "react-jsx"   
  }
}
